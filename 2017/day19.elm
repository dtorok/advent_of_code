-- SOLVER
---------
solver1 : String -> String
solver1 input = input
  |> parseInput
  |> runThrough
  |> Tuple.first

solver2 : String -> String
solver2 input = input
  |> parseInput
  |> runThrough
  |> Tuple.second
  |> (\i -> i - 1) -- minus the last step
  |> toString

type Cell = None | Tube | Cross | Letter Char
type alias Map = Array ( Array Cell )

runThrough : Map -> (String, Int)
runThrough map =
  let
    start = findStart map
    dir = (1, 0)
  in
    stepThrough start dir map ("", 0)
      |> (\(result, count) -> (String.reverse result, count))

findStart : Map -> (Int, Int)
findStart map =
  let
    find : Int -> Int
    find c =
      case mapGet (0, c) map of
        Just Tube ->
          c -- found it
        Nothing ->
          Debug.crash "Couldn't find start tube"
        _ ->
          find (c + 1) -- search more
  in
    (0, find 0)

stepThrough : (Int, Int) -> (Int, Int) -> Map -> (String, Int) -> (String, Int)
stepThrough pos dir map (result, count) =
  let
    currCell = mapGet pos map
  in
    case currCell of
      Just None ->
        (result, count + 1)
      Just Tube ->
        stepThrough (move pos dir) dir map (result, count + 1)
      Just Cross ->
        let
          newDir = findNewDir pos dir map
        in
          stepThrough (move pos newDir) newDir map (result, count + 1)
      Just (Letter ch) ->
        stepThrough (move pos dir) dir map ((String.cons ch result), count + 1)
      Nothing ->
        Debug.crash "Tube lead out of the map???"

mapGet : (Int, Int) -> Map -> Maybe Cell
mapGet (r, c) map = map
  |> Array.get r
  |> Maybe.andThen (Array.get c)

findNewDir : (Int, Int) -> (Int, Int) -> Map -> (Int, Int)
findNewDir (r, c) (vr, vc) map =
  let
    checkDir : (Int, Int) -> Maybe (Int, Int)
    checkDir (dr, dc) =
      let
        (nr, nc) = move (r, c) (dr, dc)
      in
        if (dr, dc) /= (-vr, -vc) then
          case mapGet (nr, nc) map of
            Nothing ->
              Nothing
            Just None ->
              Nothing
            _ ->
              Just (dr, dc)
        else
          Nothing
  in
    Nothing
      |> ifNothing (checkDir (1, 0))
      |> ifNothing (checkDir (-1, 0))
      |> ifNothing (checkDir (0, 1))
      |> ifNothing (checkDir (0, -1))
      |> forceJust ("Couldn't find route after cross?? " ++ (toString (r, c)))

move : (Int, Int) -> (Int, Int) -> (Int, Int)
move (r, c) (vr, vc) =
  (r + vr, c + vc)

ifNothing : Maybe a -> Maybe a -> Maybe a
ifNothing check next =
  case check of
    Nothing -> next
    Just _ -> check

forceJust : String -> Maybe a -> a
forceJust msg m =
  case m of
    Just a -> a
    Nothing -> Debug.crash msg

-- PARSER
---------
parseInput : String -> Map
parseInput input = input
  |> String.split "\n"
  |> List.map parseLine
  |> Array.fromList

parseLine : String -> Array Cell
parseLine line = line
  |> String.toList
  |> List.map parseCell
  |> Array.fromList

parseCell : Char -> Cell
parseCell char =
  case char of
    ' ' -> None
    '-' -> Tube
    '|' -> Tube
    '+' -> Cross
    _   -> Letter char

